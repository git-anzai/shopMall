<view style='box-shadow:1px 1px 6px #dfdfdf;'>
  <scroll-view scroll-x="true" style="width: 100%;white-space:nowrap;background:#fff;box-sizing: border-box;">
    <view class="tab " style='font-size:12px'>
      <view wx:for="{{tabnav.tabitem}}" bindtap="setTab" data-tabindex="{{index}}"  class="tabs-item {{showtab==index?'selected':''}}">{{item.text}}</view>
    </view>
  </scroll-view>
</view>
<view class='case-page'>
  <view class='list-masonry'>
    <view class='item-masonry' wx:for="{{note}}" bindtap='goDetail'>
      <image src='{{item.url}}' mode='widthFix'></image>
      <text style='font-size:28rpx;color:#606266'>{{item.title}}</text>
    </view>
  </view>
</view>
<view class='icon' catchtap="goAdd">
  发布
</view>